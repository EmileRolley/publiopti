{
  "name": "publiopti",
  "version": "0.1.15",
  "description": "An optimizer lib for Publicodes models",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:EmileRolley/publiopti.git",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "author": "<emile.rolley@tuta.io>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.11.18"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "jest": "^29.3.1",
    "publicodes": "^1.0.0-beta.66",
    "ts-jest": "^29.0.4",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "tsup": {
    "entry": [
      "source/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "sourceMap": true,
    "dts": true,
    "clean": true,
    "globalName": "publiopti"
  }
}
